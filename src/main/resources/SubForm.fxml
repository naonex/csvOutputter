<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SubForm">
   <VBox prefHeight="200.0" prefWidth="500.0">
      <children>
         <Label text="接続先設定">
            <VBox.margin>
               <Insets left="20.0" top="35.0" />
            </VBox.margin>
         </Label>
         <FlowPane prefHeight="0.0" prefWidth="500.0">
            <children>
               <Label text="SQL Server URL">
                  <FlowPane.margin>
                     <Insets left="20.0" />
                  </FlowPane.margin>
               </Label>
               <TextField fx:id="sqlServerUrl" prefHeight="25.0" prefWidth="350.0">
                  <FlowPane.margin>
                     <Insets left="20.0" />
                  </FlowPane.margin>
               </TextField>
            </children>
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </FlowPane>
         <FlowPane prefHeight="0.0" prefWidth="500.0">
            <children>
               <Label text="取得先テーブル名">
                  <FlowPane.margin>
                     <Insets left="20.0" />
                  </FlowPane.margin>
               </Label>
               <TextField fx:id="tableName" prefHeight="25.0" prefWidth="350.0">
                  <FlowPane.margin>
                     <Insets left="18.0" />
                  </FlowPane.margin>
               </TextField>
            </children>
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </FlowPane>
         <FlowPane prefHeight="1.0" prefWidth="500.0">
            <children>
               <Button mnemonicParsing="false" onAction="#clickReturnDefaultValuesButton" text="初期値に戻す">
                  <FlowPane.margin>
                     <Insets left="20.0" />
                  </FlowPane.margin>
               </Button>
               <Button mnemonicParsing="false" onAction="#clickReturnValidValuesButton" text="変更前に戻す">
                  <FlowPane.margin>
                     <Insets left="210.0" />
                  </FlowPane.margin>
               </Button>
               <Button mnemonicParsing="false" onAction="#clickConfirmButton" text="変更確定">
                  <FlowPane.margin>
                     <Insets left="10.0" />
                  </FlowPane.margin>
               </Button>
            </children>
            <VBox.margin>
               <Insets top="10.0" />
            </VBox.margin>
         </FlowPane>
      </children>
   </VBox>
</VBox>
