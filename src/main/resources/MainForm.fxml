<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainForm">
   <VBox prefHeight="200.0" prefWidth="500.0">
      <children>
         <Label fx:id="javaVersion" prefHeight="17.0" prefWidth="200.0" text="java version">
            <VBox.margin>
               <Insets left="280.0" top="10.0" />
            </VBox.margin>
         </Label>
         <FlowPane prefHeight="6.0" prefWidth="500.0">
            <children>
               <Label text="出力先ファイルパス">
                  <FlowPane.margin>
                     <Insets left="20.0" />
                  </FlowPane.margin>
               </Label>
               <Button mnemonicParsing="false" onAction="#clickConnectionSettingButton" text="接続先設定">
                  <FlowPane.margin>
                     <Insets left="290.0" />
                  </FlowPane.margin>
               </Button>
            </children>
            <VBox.margin>
               <Insets top="40.0" />
            </VBox.margin>
         </FlowPane>
         <TextField fx:id="filePath" prefHeight="0.0" prefWidth="460.0">
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
            </VBox.margin>
         </TextField>
         <FlowPane prefHeight="1.0" prefWidth="500.0">
            <children>
               <Button mnemonicParsing="false" onAction="#clickReferButton" text="参照">
                  <FlowPane.margin>
                     <Insets left="20.0" />
                  </FlowPane.margin>
               </Button>
               <Button mnemonicParsing="false" onAction="#clickExecButton" text="実行">
                  <FlowPane.margin>
                     <Insets left="370.0" />
                  </FlowPane.margin>
               </Button>
            </children>
         </FlowPane>
      </children>
   </VBox>
</VBox>
